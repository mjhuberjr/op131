#extend("master")
#export("title") {
    #(sectionName)
}
#export("section") {
    #loop(section, "section") {
    <h1> #(section.block.title) </h1>
    <h2> #(section.block.subtitle) </h2>
    <h3> #(section.block.description) </h3>
    <ul>
    #loop(section.links, "links") {
    #loop(links, "link") {
    <li> #(link.title) - #(link.url) </li>
    }
    }
    </ul>
    }
}
